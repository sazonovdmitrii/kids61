                <label for="newsletter">Подпишитесь на нашу рассылку</label>
            </div>
            <div class="input-box">
                <script type="text/javascript">
					function clearText(theField)
					{
    					if (theField.defaultValue == theField.value)
    					theField.value = '';
					}
					
					function addText(theField)
					{
    					if (theField.value == '')
    					theField.value = theField .defaultValue;
					}
    			</script>
               <input type="text" name="email" id="newsletter" onblur="addText(this);" onfocus="clearText(this)" value="Введите email адрес" title="Подписаться на нашу рассылку" class="input-text required-entry validate-email" />
            </div>
            <div class="actions">
                <button type="submit" title="Подписаться" class="button"><span><span>Подписаться</span></span></button>
            </div>
        </div>
    </form>
